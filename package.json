{
  "name": "yac-carousel",
  "version": "0.0.12",
  "description": "YAC Carousel (Yet Another Carousel)",
  "contributors": [
    {
      "name": "Andrej Pavlovic",
      "email": "andrej.pavlovic@massedge.com"
    }
  ],
  "license": "UNLICENSED",
  "main": "dist/yac-carousel.umd.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "develop": "webpack-dev-server --mode development --open",
    "build": "npm run clean && npm run typescript-build && npm run webpack-build",
    "prepublishOnly": "npm run build",
    "webpack-build": "webpack --config webpack.config.build.js --mode production --display-modules",
    "format": "prettier --write \"src/**/*.ts\" \"develop-pages/**/*.ts\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "tslint-fix": "tslint --fix \"src/**/*.ts\" \"develop-pages/**/*.ts\"",
    "tslint-test": "tslint --project ./test-types/ \"./test-types/**/*.ts\"",
    "typescript-build": "tsc --project tsconfig.build.json",
    "typescript-check": "tsc --noEmit",
    "typescript-watch": "tsc --noEmit --watch"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run typescript-check"
    }
  },
  "resolutions": {
    "dtslint/typescript": "^3.8.3"
  },
  "files": [
    "/dist/**/*",
    "!/dist/report.html",
    "/package.json",
    "/README.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.2",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "custom-event-polyfill": "^1.0.7",
    "dtslint": "^3.6.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "intersection-observer": "^0.10.0",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.2",
    "mini-css-extract-plugin": "^0.9.0",
    "mitt": "^1.2",
    "normalize.css": "^8.0.1",
    "prettier": "^2.0.5",
    "react": "^16.0",
    "react-dom": "^16.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "aria-tablist": "^1.2.0",
    "lodash-es": "^4.17.15"
  },
  "optionalDependencies": {
    "mitt": "^1.2",
    "react": "^16.0"
  }
}
