{
  "name": "yac-carousel",
  "version": "0.0.6",
  "description": "YAC (Yet Another Carousel) Carousel",
  "contributors": [
    {
      "name": "Andrej Pavlovic",
      "email": "andrej.pavlovic@massedge.com"
    }
  ],
  "license": "UNLICENSED",
  "main": "dist/main.min.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "develop": "webpack-dev-server --mode development --open",
    "build": "npm run clean && npm run typescript-build && npm run webpack-build",
    "prepublishOnly": "npm run build",
    "webpack-build": "webpack --mode production",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "tslint-fix": "tslint --fix \"src/**/*.ts\" \"test/**/*.ts\"",
    "typescript-build": "tsc",
    "typescript-check": "tsc --noEmit",
    "typescript-watch": "tsc --noEmit --watch"
  },
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-typescript": "^7.8.3",
    "@types/lodash-es": "^4.17.3",
    "babel-loader": "^8.0.6",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "lodash-es": "^4.17.15"
  }
}
