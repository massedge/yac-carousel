{
  "name": "yac-carousel",
  "version": "0.0.8",
  "description": "YAC (Yet Another Carousel) Carousel",
  "contributors": [
    {
      "name": "Andrej Pavlovic",
      "email": "andrej.pavlovic@massedge.com"
    }
  ],
  "license": "UNLICENSED",
  "main": "dist/main.min.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "develop": "webpack-dev-server --mode development --open",
    "build": "npm run clean && npm run typescript-build && npm run webpack-build",
    "prepublishOnly": "npm run build",
    "webpack-build": "webpack --mode production --display-modules",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "tslint-fix": "tslint --fix \"src/**/*.ts\" \"test/**/*.ts\"",
    "tslint-test": "tslint --project ./test-types/ \"./test-types/**/*.ts\"",
    "typescript-build": "tsc --project tsconfig.build.json",
    "typescript-check": "tsc --noEmit",
    "typescript-watch": "tsc --noEmit --watch"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run typescript-check"
    }
  },
  "resolutions": {
    "dtslint/typescript": "^3.8.3"
  },
  "files": [
    "dist/**/*",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.1.4",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^13.9.5",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.4",
    "custom-event-polyfill": "^1.0.7",
    "dtslint": "^3.3.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.3",
    "intersection-observer": "^0.7.0",
    "jest": "^25.2.4",
    "lint-staged": "^10.0.8",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.1",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "aria-tablist": "^1.2.0",
    "lodash-es": "^4.17.15",
    "mitt": "^1.2.0"
  }
}
